# PATCHIT-only compose env (copy to `deploy/.env` locally; do NOT commit your real `.env`)

# Optional (Bytez agent service)
BYTEZ_API_KEY=

# Inhouse agent endpoint (recommended for company use)
PATCHIT_INHOUSE_AGENT_URL=
PATCHIT_INHOUSE_API_KEY=
PATCHIT_INHOUSE_HEADERS_JSON=

# Host ports (if 8088/8089 are already used by another stack)
PATCHIT_PORT=8088
PATCHIT_UI_PORT=8089
PATCHIT_AGENT_PORT=8098

# Optional: if you want GitHub PRs (otherwise use Local worktree mode)
PATCHIT_GITHUB_MODE=mock
PATCHIT_GITHUB_TOKEN=
PATCHIT_GITHUB_REPO=

# Optional: Airflow poller (if you want PATCHIT to discover failures by polling Airflow API)
PATCHIT_AIRFLOW_POLLER_ENABLED=false
PATCHIT_AIRFLOW_BASE_URL=
PATCHIT_AIRFLOW_USERNAME=
PATCHIT_AIRFLOW_PASSWORD=
